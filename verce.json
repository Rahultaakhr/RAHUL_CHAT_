{
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/$1" }
    ],
    "cleanUrls": true,
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/chatsection",
        "dest": "/index.html"
      },
      {
        "src": "/login",
        "dest": "/index.html"
      },
      {
        "src": "/signup",
        "dest": "/index.html"
      },
      {
        "src": "^(?!\\/api\\/)",
        "dest": "/index.html"
      }
    ]
  }
  